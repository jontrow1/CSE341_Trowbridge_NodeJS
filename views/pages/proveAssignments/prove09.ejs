<!DOCTYPE html>
<html>
<head>
  <% include ../../includes/header.ejs %>
  <link rel="stylesheet" href="/stylesheets/prove09.css">
</head>

<body>
  <% include ../../includes/nav.ejs %>
<div class="jumbotron text-center">
  <div class="container">
    <h1><%= title %></h1>   
  </div>
</div>
<div class="container">
  <% pokemonList.forEach( pokemon => { %>
    <div class="col-5 m-0 p-0 pt-2 pb-2 pokemon"><h4><%=pokemon.name.toUpperCase()%></h4></div>
<% }); %>
</div>

<div class="centered">
<% if(page != 1) { %>
    <a href="http://localhost:5000/proveAssignments/prove09/1"><button class="btn btn-secondary m-2">First</button></a>
    <a href="http://localhost:5000/proveAssignments/prove09/<%=parseInt(page) - 1%>"><button class="btn btn-secondary m-2">Previous</button></a>
<% } %>

<button class="btn btn-primary m-2"><%=page%></button>

<% if(page != 112) { %>
    <a href="http://localhost:5000/proveAssignments/prove09/<%=parseInt(page) + 1%>"><button class="btn btn-secondary m-2">Next</button></a>
    <a href="http://localhost:5000/proveAssignments/prove09/112"><button class="btn btn-secondary m-2">Last</button></a>
<% } %>
</div>

</body>
</html>